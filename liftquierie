t=int(input())
A=0
B=7
for m in range(t):
    x=int(input())
    Y=abs(A-x)
    Z=abs(B-x)
    if(Y>Z):
        print("B")
        B=x
    if(Y<Z):
        print("A")
        A=x
    if(Y==Z):
        if(A<B):
            print("A")
            A=x
        else:
            print("B")
            B=x
